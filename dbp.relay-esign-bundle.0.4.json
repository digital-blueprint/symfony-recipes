{
    "manifests": {
        "dbp/relay-esign-bundle": {
            "manifest": {
                "bundles": {
                    "Dbp\\Relay\\EsignBundle\\DbpRelayEsignBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "PDF_AS_WEB_ADVANCED_URI": "https://pdfas-dev.tugraz.at/pdf-as-web",
                    "PDF_AS_WEB_QUALIFIED_URI": "https://sig-dev.tugraz.at/pdf-as-web"
                }
            },
            "files": {
                "config/packages/dbp_relay_esign.yaml": {
                    "contents": [
                        "dbp_relay_esign:",
                        "    qualified_signature:",
                        "        server_url: '%env(PDF_AS_WEB_QUALIFIED_URI)%'",
                        "        profiles:",
                        "            - name: default",
                        "              profile_id: SIGNATUREBLOCK_PROFILE1",
                        "              user_text:",
                        "                target_table: usercontent",
                        "                target_row: 1",
                        "                attach:",
                        "                    parent_table: main",
                        "                    child_table: user",
                        "                    parent_row: 2",
                        "              role: ROLE_SCOPE_PROFILE1",
                        "    advanced_signature:",
                        "        server_url: '%env(PDF_AS_WEB_ADVANCED_URI)%'",
                        "        profiles:",
                        "            - name: official",
                        "              key_id: profile2-key",
                        "              profile_id: SIGNATUREBLOCK_profile2",
                        "              user_text:",
                        "                target_table: usercontent",
                        "                target_row: 1",
                        "                attach:",
                        "                    parent_table: main",
                        "                    child_table: user",
                        "                    parent_row: 2",
                        "              role: ROLE_SCOPE_profile2",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "62eb2deb94d284eac7ef5ff1bceb72aa593767a0"
        }
    }
}
